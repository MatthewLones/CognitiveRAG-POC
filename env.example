# Environment variables for Cognitive RAG POC
# Copy this file to .env and fill in your API keys

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Model configuration
DEFAULT_LLM_MODEL=gpt-4-turbo-preview
DEFAULT_EMBEDDING_MODEL=all-MiniLM-L6-v2

# Performance settings
MAX_TOKENS=4000
TEMPERATURE=0.1
TIMEOUT=30

# Hugging Face tokenizers settings (prevents parallelism warnings)
TOKENIZERS_PARALLELISM=false

# Data paths
DATA_DIR=data
CONFIG_DIR=configs

# API settings
API_HOST=0.0.0.0
API_PORT=8000
UI_PORT=8501
